<div id="legende" title="{_T string="Legend"}">
    <h1>{_T string="Legend"}</h1>
    <table>
        <tr>
            <th>
                <img src="{path_for name="plugin_res" data=["plugin" => $module_id, "path" => "leaflet-1.2.0/images/marker-galette.png"]}" alt="{_T string="Member"}"/>
            </th>
            <td>
                {_T string="Member"}
            </td>
        </tr>
        <tr>
            <th>
                <img src="{path_for name="plugin_res" data=["plugin" => $module_id, "path" => "leaflet-1.2.0/images/marker-galette-pro.png"]}" alt="{_T string="Member (company)" domain="maps"}"/>
            </th>
            <td>
                {_T string="Member (company)" domain="maps"}
            </td>
        </tr>
        <tr>
            <th>
                <img src="{path_for name="plugin_res" data=["plugin" => $module_id, "path" => "leaflet-1.2.0/images/marker-icon.png"]}" alt="{_T string="Search result" domain="maps"}"/>
            </th>
            <td>
                {_T string="Search result" domain="maps"}
            </td>
        </tr>
    </table>
</div>
